{
  "name": "water-wazi-landing-site",
  "version": "1.0.0",
  "repository": "git@github.com:OpenUpSA/water-wazi-landing-site.git",
  "author": "JD Bothma <jbothma@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "20.x"
  },
  "private": false,
  "importWebflowExport": {
    "copyTrees": {
      "css": "src/css",
      "js": "src/js",
      "images": "src/images"
    },
    "importHtml": [
      {
        "glob": "*.html",
        "destDir": "src",
        "transforms": "./src/js/webflow/import.js"
      }
    ]
  },
  "scripts": {
    "dev": "npx serve src"
  }
}
